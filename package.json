    "@storybook/react": "5.2.1",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.3",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@typescript-eslint/eslint-plugin": "2.0.0",
    "@typescript-eslint/parser": "2.0.0",
    "babel-loader": "8.0.6",
    "date-fns": "2.2.1",
    "eslint-config-prettier": "6.1.0",
    "eslint-plugin-react-hooks": "2.0.1",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-scripts": "3.1.2",
    "typescript": "3.6.2"
